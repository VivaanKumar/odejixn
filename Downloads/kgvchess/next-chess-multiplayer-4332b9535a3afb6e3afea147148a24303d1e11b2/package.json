    "concurrently": "^7.6.0",
    "encoding": "^0.1.13",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.4",
    "firebase": "^8.10.0",
    "moment": "^2.29.4",
    "next": "13.1.4",
    "next-sitemap": "^3.1.52",
    "nextjs-sitemap-generator": "^1.3.1",
    "react": "18.2.0",
    "react-chessboard": "^2.0.8",
    "react-dom": "18.2.0",
